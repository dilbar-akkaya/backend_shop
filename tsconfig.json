{
    "compilerOptions": {
      "module": "commonjs",
      "outDir": "dist",
      "target": "es2017",
      "noImplicitAny": true,
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "sourceMap": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }